<view class="container">
  <view class="page-head">
    <view class="page-head-title" bindtap="targetModifyGroup">{{groupName}}</view>
    <view class="page-head-line"></view>
  </view>
  <view class="page-body">
    <view class="page-section">
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd">
              <navigator class="navigator" url="/pages/share/shareinfo/shareinfo" hover-class="navigator-hover">
                <view><text class="accountinfo_project">买水果买水果买水果买水</text>
                </view>
                <view><text class="accountinfo_money">10000000元</text>
                </view>
                <view><text class="accountinfo_status">已结</text>
                </view>
                <image class="navigator_arrow" src="/pages/resources/arrow.png" background-size="cover" />
              </navigator>
            </view>
          </view>
        </view>
      </view>
  </view>
  <view class="flex fix_nav_wp">
    <view class="nav_link">
      <button class="defalut_btn">
        <view class="plus_wp">
        <image src='/pages/resources/plus_ico.png' class="plus_ico" bindtap="toCreateDetail" />
      </view>
      <text class="txt txt_fb">记一笔</text>
      </button>
    </view>
  </view>
</view>
