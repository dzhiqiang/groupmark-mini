<view class="container">
  <view class="page-body">
    <view class="page-section">
      <view class="page-section-title">20191001-20191010</view>
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__bd">
            <navigator class="navigator" url="/pages/share/share" hover-class="navigator-hover">
              <text class="navigator_name">总金额1000元 25笔</text>
              <image class="navigator_arrow" src="/pages/resources/arrow.png" background-size="cover" />
            </navigator>
          </view>
        </view>
      </view>
    </view>
    <view class="page-section">
      <view class="page-section-title">出款人</view>
      <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-check__label" wx:for="{{items}}" wx:key="{{item.value}}">
            <view class="weui-cell__bd">{{item.name}}</view>
            <view class="weui-cell__bd">
              <input class="weui-input"  name="input" value="100" disabled="true" />
            </view>
          </view>
      </view>
    </view>
    <view class="page-section">
        <view class="page-section-title">收支</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-check__label" wx:for="{{items}}" wx:key="{{item.value}}">
            <view class="weui-cell__bd">{{item.name}}</view>
            <view class="weui-cell__bd">收</view>
            <view class="weui-cell__bd">
              <input class="weui-input"  name="input" value="100" disabled="true" />
            </view>
          </view>
        </view>
    </view>
  </view> 
</view>